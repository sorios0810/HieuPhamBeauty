<!DOCTYPE html>
<html lang="en">
<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>Hiếu Phạm Beauty & Acedamny</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">

    <!--== Favicon ==-->
    <link rel="shortcut icon" href="../../Content/img/favicon.ico" type="image/x-icon" />

    <!--== Google Fonts ==-->
    <link rel="preconnect" href="https://fonts.googleapis.com/">
    <link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700;800;900&amp;display=swap" rel="stylesheet">
    @Styles.Render("~/Content/css/lib")
    <!--== Main Style CSS ==-->
    <link href="../../Content/css/style.css" rel="stylesheet" />
</head>

<body>
    <!--wrapper start-->
    <div class="wrapper home-default-wrapper">
        <!--== Start Preloader Content ==-->
        <div class="preloader-wrap">
            <div class="preloader">
                <span class="dot"></span>
                <div class="dots">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </div>
        </div>
        <!--== End Preloader Content ==-->
        @Html.Partial("_HeaderPartial")
        @RenderBody()
        @Html.Partial("_FooterPartial")
        <!--== Scroll Top Button ==-->
        <div id="scroll-to-top" class="scroll-to-top"><span class="ion-md-arrow-up"></span></div>

        <!--== Start Side Menu ==-->
        <aside class="off-canvas-wrapper">
            <div class="off-canvas-inner">
                <div class="off-canvas-overlay"></div>
                <!-- Start Off Canvas Content Wrapper -->
                <div class="off-canvas-content">
                    <!-- Off Canvas Header -->
                    <div class="off-canvas-header">
                        <div class="close-action">
                            <button class="btn-menu-close">menu<i class="icon-arrow-left"></i></button>
                        </div>
                    </div>

                    <div class="off-canvas-item">
                        <!-- Start Mobile Menu Wrapper -->
                        <div class="res-mobile-menu menu-active-one">
                            <!-- Note Content Auto Generate By Jquery From Main Menu -->
                        </div>
                        <!-- End Mobile Menu Wrapper -->
                    </div>
                </div>
                <!-- End Off Canvas Content Wrapper -->
            </div>
        </aside>
        <!--== End Side Menu ==-->
    </div>

    <!--=======================Javascript============================-->
    <!--=== Modernizr Min Js ===-->
    <script src="../../Scripts/lib/modernizr.js"></script>
    <!--=== jQuery Min Js ===-->
    <script src="../../Scripts/lib/jquery-main.js"></script>
    <!--=== jQuery Migration Min Js ===-->
    <script src="../../Scripts/lib/jquery-migrate.js"></script>
    <!--=== Bootstrap Min Js ===-->
    <script src="../../Scripts/lib/bootstrap.min.js"></script>
    <!--=== jQuery Appear Js ===-->
    <script src="../../Scripts/lib/jquery.appear.js"></script>
    <!--=== jQuery Swiper Min Js ===-->
    <script src="../../Scripts/lib/swiper.min.js"></script>
    <!--=== jQuery Fancy Box Min Js ===-->
    <script src="../../Scripts/lib/fancybox.min.js"></script>
    <!--=== jQuery Slick Nav Js ===-->
    <script src="../../Scripts/lib/slicknav.js"></script>
    <!--=== jQuery Waypoints Js ===-->
    <script src="../../Scripts/lib/waypoints.js"></script>
    <!--=== jQuery Owl Carousel Min Js ===-->
    <script src="../../Scripts/lib/owlcarousel.min.js"></script>
    <!--=== jQuery Match Height Min Js ===-->
    <script src="../../Scripts/lib/jquery-match-height.min.js"></script>
    <!--=== jQuery Zoom Min Js ===-->
    <script src="../../Scripts/lib/jquery-zoom.min.js"></script>
    <!--=== Countdown Js ===-->
    <script src="../../Scripts/lib/countdown.js"></script>

    <!--=== Custom Js ===-->
    <script src="../../Scripts/custom.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
